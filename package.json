{
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:app": "pnpm -C packages/app run build",
    "build:core": "pnpm -C packages/core run build",
    "build:core-test": "pnpm -C packages/core-test run build",
    "build:examples": "pnpm -C packages/examples run build"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "dprint": "^0.49.1",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.30.1"
  },
  "pnpm": {
    "overrides": {
      "tanebi": "workspace:*"
    }
  },
  "packageManager": "pnpm@10.8.0"
}
